  "author": "Jack Baron <jackmbaron@gmail.com> (https://www.jackbaron.com)",
  "license": "ISC",
  "private": true,
  "scripts": {
    "dev": "parcel ./src/index.html",
    "clean": "rimraf .cache dist",
    "build": "parcel build ./src/index.html",
    "build:clean": "yarn run clean && parcel build ./src/index.html",
    "build:prod": "yarn run clean && parcel build --public-url . --no-source-maps ./src/index.html",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "check": "tsc --noEmit",
    "test": "yarn run lint && yarn run check"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-transform-runtime": "^7.4.4",
