    "@types/dateformat": "^3.0.0",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@types/react-redux": "^7.0.9",
    "@types/react-router-dom": "^4.3.3",
    "parcel-bundler": "^1.12.3",
    "prettier": "^1.17.1",
    "rimraf": "^2.6.3",
    "sass": "^1.20.3",
    "tslint": "^5.17.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.5.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.5",
