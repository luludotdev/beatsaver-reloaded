    "react-dom": "^16.8.6",
    "react-intersection-observer": "^8.23.0",
    "react-redux": "^7.0.3",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "sweetalert2": "^8.11.7",
    "sweetalert2-react-content": "^1.1.0"
